{
  "name": "type-chef-di",
  "version": "0.0.1",
  "description": "dependency injection container.",
  "main": "build/index.js",
  "types": "build",
  "keywords": [
    "dependency-injection",
    "di",
    "container"
  ],
  "scripts": {
    "build": "tsc -p .",
    "start": "ts-node-dev lib/app.ts",
    "test": "jest --silent --verbose --coverage",
    "local-publish": "rm -rf ./build && npm run generate-index && npm version patch --force && npm run build  && cp *.json ./build && cp *.md && npm publish ./build --registry http://localhost:4873/",
    "generate-index": "cti clean ./lib && cti create ./lib"
  },
  "author": "Maurer Krisztian",
  "license": "ISC",
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.0.23",
    "@types/reflect-metadata": "^0.1.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.52",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenZer0/type-chef-di.git"
  },
  "bugs": {
    "url": "https://github.com/OpenZer0/type-chef-di/issues"
  },
  "homepage": "https://github.com/OpenZer0/type-chef-di#readme"
}
